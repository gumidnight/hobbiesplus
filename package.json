{
  "name": "hobbies-plus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2024-09-23 --compatibility-flags=nodejs_compat",
    "pages:deploy": "npx wrangler pages deploy .vercel/output/static",
    "db:migrate:local": "npx wrangler d1 execute hobbies-plus-db --local --file=./migrations/0001_create_registrations.sql",
    "db:migrate:remote": "npx wrangler d1 execute hobbies-plus-db --remote --file=./migrations/0001_create_registrations.sql"
  },
  "dependencies": {
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "jose": "^5.2.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.0",
    "@cloudflare/workers-types": "^4.20241004.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.0",
    "wrangler": "^3.99.0"
  }
}
